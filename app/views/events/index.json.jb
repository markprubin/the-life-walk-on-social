@events.map do |event|
  {
    id: event.id,
    name: event.name,
    description: event.description,
    address: event.address,
    image_url: event.image_url,
    start_time: event.start_event,
    end_time: event.end_event,
    favorites: event.favorite_count,
    favorites_list: event.favorites
  }
end